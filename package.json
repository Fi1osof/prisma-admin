{
  "name": "prisma-admin",
  "version": "0.0.2",
  "author": "Ahmed Elywa",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate": "yarn generate:prisma && yarn generate:nexus && yarn codegen",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/Api/nexusSchema",
    "codegen": "graphql-codegen --config codegen.yml",
    "create-migration": "prisma migrate save --experimental",
    "migrate-database": "prisma migrate up --experimental --auto-approve --create-db"
  },
  "dependencies": {
    "@apollo/client": "3.3.5",
    "@apollo/react-ssr": "4.0.0",
    "@nexus/schema": "0.19.2",
    "@paljs/nexus": "2.9.1",
    "@paljs/admin": "2.9.0",
    "@paljs/ui": "1.0.4",
    "@paljs/icons": "1.0.4",
    "@prisma/client": "2.13.0",
    "apollo-server-micro": "2.19.0",
    "bcryptjs": "2.4.3",
    "cookie": "0.4.1",
    "graphql": "15.4.0",
    "graphql-tag": "2.11.0",
    "graphql-tools": "7.0.2",
    "jsonwebtoken": "8.5.1",
    "next": "10.0.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "5.2.1",
    "ts-node": "9.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@prisma/cli": "2.13.0",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie": "0.4.0",
    "@types/graphql": "14.5.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "14.14.12",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/styled-components": "5.1.5",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "babel-plugin-graphql-tag": "3.1.0",
    "babel-plugin-styled-components": "1.12.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-react": "7.21.5",
    "husky": "4.3.5",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "typescript": "4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --pattern \"**/*.*(ts|tsx|graphql)\" --staged"
    }
  },
  "cacheDirectories": [
    ".next/cache"
  ],
  "repository": "https://github.com/AhmedElywa/prisma-admin",
  "description": "Full stack Prisma, GraphQL, Nexus, Apollo and NextJS with Admin UI to control your Prisma db"
}
